{
  "mcpServers": {
    "searxng": {
      "command": "npx",
      "args": [
        "-y",
        "mcp-searxng"
      ],
      "env": {
        "SEARXNG_URL": "https://searx.salonek.org/"
      },
      "disabled": false,
      "autoApprove": [],
      "alwaysAllow": [
        "searxng_web_search",
        "web_url_read"
      ],
      "disabledTools": []
    },
    "context7": {
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ],
      "env": {
        "DEFAULT_MINIMUM_TOKENS": ""
      },
      "alwaysAllow": [
        "resolve-library-id",
        "query-docs"
      ],
      "disabled": false
    },
    "github": {
      "type": "streamable-http",
      "url": "https://api.githubcopilot.com/mcp",
      "headers": {
        "Authorization": "Bearer <TOKEN>"
      },
      "alwaysAllow": [
        "get_me",
        "pull_request_read"
      ],
      "disabled": true
    }
  }
}
